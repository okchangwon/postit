{
  "env": {
    "browser": true,
    "es6": true
  },
  "globals": {
    "$": false
  },
  "extends": "airbnb-base",
  "rules": {
    "semi": [0, "always"],
    "quotes": [0, "double"],
    "no-console": 0,
    "no-underscore-dangle": 0,
    "eol-last": 0,
    "class-methods-use-this": 0,
    "no-useless-constructor": 0,
    "comma-dangle": 0,
    "no-mixed-operators": 0,
    "no-unused-expressions": 0,
    "arrow-parens": 0,
    "no-param-reassign": 0,
    "prefer-destructuring": 0,
    "newline-per-chained-call": 0,
    "no-plusplus": 0,
    "max-len": 0
  },
  "parserOptions": {
    "sourceType": "module"
  }
}